@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h4>Information received from Okta</h4>
<br />

<table class="table">
    <thead>
        <th>Key</th>
        <th>Value</th>
    </thead>
    <tbody>

        @{
            if(Model.userinfo != null)
            {
                foreach(var item in Model.userinfo)
                {
                    <tr>
                        <td>@item.Key</td>
                        <td>@item.Value</td>
                    </tr>                   
                    
                }
            }
        }
        
    </tbody>
</table>


